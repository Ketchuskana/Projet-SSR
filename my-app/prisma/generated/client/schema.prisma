generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  name      String
  email     String   @unique
  telephone String
  message   String
  completed Boolean  @default(false)
}

model Projet {
  id               Int      @id @default(autoincrement())
  name             String
  description      String
  contexte         String
  technologies     String
  resultats        String
  objectifs        String
  client           String
  date_realisation DateTime
  completed        Boolean  @default(false)
  images           String
  videos           String
  testimonials     String
}
